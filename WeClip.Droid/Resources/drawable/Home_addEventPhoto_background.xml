<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
  <item>
    <shape
        android:shape="oval">
      <solid
          android:color="#02C9C8"/>
    </shape>
  </item>

  <item
    android:left="5dp"
    android:right="5dp"
    android:top="5dp"
    android:bottom="5dp"
    >
    <shape
            android:shape="oval">
      <stroke android:width="5dp"
          android:color="#FFFFFF"/>
    </shape>
  </item>
 
</layer-list>